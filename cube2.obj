# Created by Morgan McGuire and released into the Public Domain on
# July 16, 2011.
#
# http://graphics.cs.williams.edu/data

mtllib default.mtl

v -0.5 0.5 -0.5
v -0.5 0.5 0.5
v 0.5 0.5 0.5
v 0.5 0.5 -0.5
v -0.5 -0.5 -0.5
v -0.5 -0.5 0.5
v 0.5 -0.5 0.5
v 0.5 -0.5 -0.5

vt 0 1
vt 0 0
vt 1 0
vt 1 1

vn 0 1 0
vn -1 0 0
vn 1 0 0
vn 0 0 -1
vn 0 0 1
vn 0 -1 0

g cube
usemtl default

f -8/-4 -7/-3 -6/-2
f -8/-4 -6/-2 -5/-1
f -8/-4 -4/-3 -3/-2
f -8/-4 -3/-2 -7/-1
f -6/-4 -2/-3 -1/-2
f -6/-4 -1/-2 -5/-1
f -5/-4 -1/-3 -4/-2
f -5/-4 -4/-2 -8/-1
f -7/-4 -3/-3 -2/-2
f -7/-4 -2/-2 -6/-1
f -3/-4 -4/-3 -1/-2
f -3/-4 -1/-2 -2/-1
